{
  "root": true,
  "env": {
    "browser": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "extends": [
    "../.eslintrc.json"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "vue/html-closing-bracket-newline": "off",
    "vue/singleline-html-element-content-newline": "off"
  }
}
